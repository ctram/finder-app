<div id='content' ng-app='finderApp'>
  <h1>The Finder-er App!</h1>

  <div ng-controller='ResultsListController as resultsList'>
    <form ng-submit='resultsList.submitSearch()'>
      Please type in a location or interest:
      <br><br>
      <input type='text' placeholder='Mountain View, Pekingese Puppies, Spelunking...' id='input-box'>
      <br><br>
      <input type='submit' value='Go!' class='btn btn-primary'>
    </form>
  </div>

  <hr>

  <div class='list-wrapper'>
    <ul id='list'>
    </ul>
  </div>

  <div id='map'>
  </div>
</div>
